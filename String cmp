#include <stdio.h>
#include <string.h>

int main() {
    char str1[50] = "Apple";
    char str2[50] = "Banana";

    int result = strcmp(str1, str2);

    if(result == 0)
        printf("Strings are equal");
    else if(result > 0)
        printf("str1 is greater");
    else
        printf("str1 is smaller");

    return 0;
}
